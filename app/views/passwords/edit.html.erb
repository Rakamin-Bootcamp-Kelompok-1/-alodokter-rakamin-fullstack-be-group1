<h1>Reset Password</h1>

<%= form_with do |f| %>
  <div class='field'>
    <%= f.label :email %> 
    <br/>
    <%= f.email_field :email %>
     <br/>
    <%= f.label :password %>
     <br/>
    <%= f.password_field :password %>
   
  </div>
   <br/>
   <div class='actions'><%= f.submit 'Update password' %></div>
<% end %>

